<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soorajmohan.test.allyoucaneatapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AllYouCanEatApplication">
        <activity
            android:name=".UpdateOrderItem"
            android:exported="true"
            android:parentActivityName=".PreviousOrderItems"/>
        <activity
            android:name=".PreviousOrderItems"
            android:exported="true"
            android:parentActivityName=".PreviousOrder"/>
        <activity
            android:name=".PreviousOrder"
            android:exported="true"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Cart"
            android:exported="true"
            android:parentActivityName=".NewOrder" />
        <activity
            android:name=".AddToOrder"
            android:exported="true"
            android:parentActivityName=".NewOrder" />
        <activity
            android:name=".NewOrder"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>